FoamFile
{
    version     3.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      porousFoamDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Numbers of fields
nC                      1;      // Number of concentrations (CList)
nR                      1;      // Number of reactions (RList, dRList)

// Solver flags
QCON                    false;  // Constant flow rate
DEBUG                   true;   // Include debugging info

// Solver parameters
Qmax                    1e6;    // Max volumetric flow rate

